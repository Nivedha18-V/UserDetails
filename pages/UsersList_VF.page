<apex:page controller="UserDashBoard" action="{!getAllUsers}" standardStylesheets="false" showHeader="false" sidebar="false">
    <style>
        table {
        border-collapse: collapse;
        width: 100%;
        }
        
        th, td {
        text-align: left;
        padding: 8px;
        }
        
        tr:nth-child(even){background-color: #f2f2f2}
        
        th {
        background-color: #4CAF50;
        color: white;
        }
    </style>
    <apex:form >
        <table>
            
            <tr><th>Name</th><th>Email</th><th>Username</th><th>Password</th><th>CreatedDate</th></tr>
            <apex:repeat value="{!users}" var="alluser">
                <tr><td><apex:outputText value="{!alluser.Name}"/></td>
                    <td><apex:outputText value="{!alluser.Email__c}"/></td>
                    <td><apex:outputText value="{!alluser.username__c}"/></td>
                    <td><apex:outputText value="{!alluser.Passwordd__c}"/></td>
                    <td><apex:outputText value="{!alluser.CreatedDate}"/></td></tr>
                
            </apex:repeat>
            
        </table>
    </apex:form>
</apex:page>